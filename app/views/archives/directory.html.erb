<article class="row">
  <h3 class="text-center archive">
    ARCHIVE
</h3>

<section class="rotatimg center-block">
<%= link_to image_tag("#{@artifact.image}", alt: "#{@artifact.alt}", class: "img-responsive"), 
  @artifact.path , data: { no_turbolink: true }, class: "archive-link"%>
  </section>
</article>

<section class="swap-images">

  <%@artifacts.each do |artifact| %>

    <%= link_to image_tag("#{artifact.image}", alt: "#{artifact.alt}", class: "img-responsive"), 
    artifact.path, data: { no_turbolink: true }, class: "archive-link"%>

  <% end %>

</section>



